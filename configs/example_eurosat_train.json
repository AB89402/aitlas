{
    "model": {
        "classname": "aitlas.models.ResNet50",
        "config": {
            "num_classes": 10,
            "learning_rate": 0.001
        }
    },
    "dataset": {
        "classname": "aitlas.datasets.EurosatDataset",
        "config": {
            "mode": "rgb",
            "root": "./data/EuroSAT/2750",
            "override": true,
            "split": {
                "train": {
                    "ratio": 80,
                    "file": "./data/train.csv"
                },
                "val": {
                    "ratio": 10,
                    "file": "./data/val.csv"
                },
                "test": {
                    "ratio": 10,
                    "file": "./data/test.csv"
                }
            }
        }
    },
    "task": {
        "classname": "aitlas.tasks.TrainTask",
        "config": {
            "epochs": 100,
            "model_directory": "./examples/experiment",
            "save_epochs": 10
        }
    }
  }
