{
    "model": {
        "classname": "aitlas.models.CifarModel",
        "config": {
            "num_classes": 10,
            "learning_rate": 0.001
        }
    },
    "dataset": {
        "classname": "aitlas.datasets.CifarDataset",
        "config": {
            "download": true,
            "train": true
        }
    },
    "task": {
        "classname": "aitlas.tasks.TrainTask",
        "config": {
            "epochs": 100,
            "model_directory": "./examples/experiment",
            "save_epochs": 10
        }
    }
  }
