{
    "model": {
        "classname": "aitlas.models.ShallowCNNNet",
        "config": {
            "num_classes": 10,
            "learning_rate": 0.001
        }
    },
    "task": {
        "classname": "aitlas.tasks.PredictTask",
        "config": {
            "model_path": "./examples/experiment/eurosat/checkpoint.pth.tar",
            "dir": "./data/test/",
            "output_path": "./examples/",
            "output_format": "plot",
            "dataset_config":{
                "classname": "aitlas.datasets.EurosatDataset",
                "config": {
                    "transforms": "aitlas.transforms.DefaultTransforms"
                }
            }
        }
    }
}
